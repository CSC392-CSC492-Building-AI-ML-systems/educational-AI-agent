[5934.220623, "o", "m/mounted-ext3fs\u001b[K\r\n\u001b[K\n    \u001b[33m# hide some directories before copying the nfsroot \u001b[39m                                                                                                                                                                                        \r\n\u001b[A\n    customize_nfsroot\u001b[K\r\n    calculate_required_size\u001b[K\r\n\u001b[K\n    \u001b[33m# creates an empty file \u001b[39m                                                                                                                                                                                                                   \r\n\u001b[A\n    dd \u001b[33mif\u001b[39m=/dev/zero \u001b[33mof\u001b[39m=$\u001b[33mtmp\u001b[39m/$\u001b[33mliveos\u001b[39m/ext3fs.img \u001b[33mbs\u001b[39m=1MB \u001b[33mcount\u001b[39m=$\u001b[33mcount\u001b[39m 2>/dev/null ||\u001b[K\r\n        die 9 \u001b[32m\"dd failed. Maybe no enough free space in $tmp\"\u001b[39m\u001b[K\r\n\u001b[K\n    mkfs.ext4 -m1 -q -O^has_journal -F -L FAI-NFSROOT $\u001b[33mtmp\u001b[39m/$\u001b[33mliveos\u001b[39m/ext3fs.img\u001b[K\r\n\u001b[K\n    \u001b[33m# the resulting ext3fs.img is mounted \u001b[39m                                                                                                                                                                                                     \r\n\u001b[A\n    mount -o loop,noatime $\u001b[33mtmp\u001b[39m/$\u001b[33mliveos\u001b[39m/ext3fs.img $\u001b[33mtmp\u001b[39m/$\u001b[33mliveos\u001b[39m/mounted-ext3fs\u001b[K\r\n\u001b[K\n    \u001b[1m\u001b[34mecho\u001b[0m \u001b[32m\"Copying the nfsroot to CD image\"\u001b[39m\u001b[K\r\n    \u001b[33m# copy the contents of the nfsroot into it \u001b[39m                                                                                                                                                                                                \r\n\u001b[A\n    cp -